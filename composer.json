{
	"name": "jumilla/laravel-source-generator",
	"type": "library",
	"description": "PHP source code generator. Can be written using a method chain.",
	"license": "MIT",
	"keywords": ["php", "source", "code", "generator"],
	"homepage": "http://jumilla.me",
	"authors": [
		{
			"name": "Fumio Furukawa",
			"email": "fumio@jumilla.me"
		}
	],
	"require": {
        "php": "^7.0",
		"illuminate/console": "^5.5",
		"illuminate/support": "^5.5",
		"jumilla/source-generator": "^1.0"
	},
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
	"autoload": {
		"psr-4": {
			"Jumilla\\Generators\\Laravel\\": "sources/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
